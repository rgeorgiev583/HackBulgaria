package com.hackbulgaria.corejava.jaxb;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class Student {
	private int studentId;
	private String name;
	
	private Student() {
	}
	
	public Student(int studentId, String name) {
		this.studentId = studentId;
		this.name = name;
	}
}
