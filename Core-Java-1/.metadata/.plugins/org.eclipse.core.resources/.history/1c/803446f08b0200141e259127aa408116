public class ASCIIArtViewer {
	BufferedImage image;
	
	public static void main(String[] args) {
		Charset charset = Charset.forName("ASCII");
		ImageInputStream is = ImageIO.createImageInputStream(args[0]);
		image = ImageIO.read(Files.);
		
	}
}
